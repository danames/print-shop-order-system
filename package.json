    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "express-rate-limit": "^7.1.5",
    "socket.io": "^4.7.4",
    "uuid": "^9.0.1",
    "moment": "^2.29.4",
    "express-validator": "^7.0.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.2"
  },
  "keywords": ["print-shop", "order-management", "display", "react", "nodejs"],
