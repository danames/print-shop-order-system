{
  "name": "print-shop-order-system",
  "version": "0.1.0",
  "description": "Print shop order management system with TV display, admin panel, and public order form",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "build": "npm run build:frontend",
    "build:frontend": "cd src/frontend && npm run build",
    "install:frontend": "cd src/frontend && npm install"
  },
  "dependencies": {
